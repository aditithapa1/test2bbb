<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="453.0" prefWidth="526.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.test2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField text fx:id="productname"  />
   <TextField text fx:id="productprice" />
   <TextField text fx:id="orderdate" /><

    ><Label fx:id="welcomeText" />
   <Button mnemonicParsing="false" onAction="#updatedata" text="update" />
   <Button mnemonicParsing="false" onAction="#deletedata" text="delete" />
   <Button mnemonicParsing="false" onAction="#insertdata" prefHeight="15.0" prefWidth="52.0" text="insert" />
    <Button onAction="#onHelloButtonClick" prefHeight="0.0" prefWidth="47.0" text="click" />
</VBox>
